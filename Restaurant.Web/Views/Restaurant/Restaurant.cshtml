@model IEnumerable<BusinessObjects.RestaurantBO>
@{
    ViewBag.Title = "Restaurant";
}

<div class="container mt-3">

    <div class="p-2 pt-md-4 pb-md-3 mx-auto text-center">
        <h1 class="display-4">Restaurant Details</h1>
    </div>
    <table style="width:100%" class="table table-bordered table-dark table-hover table-striped">
    <thead>
        <tr>
            <th>
                Restaurant Name
            </th>
            <th>
               Address
            </th>
            <th >
                Mobile Number
            </th>
        </tr>
    </thead>
    @foreach (var restaurant in Model)
    {
        <tr>
            <td>
                @restaurant.RestaurantName
            </td>
            <td>
                @restaurant.Address
            </td>
            <td>
                @restaurant.MobileNo
            </td>
           
        </tr>

    }
    </table>
</div>


